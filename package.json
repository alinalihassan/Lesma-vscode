{
    "name": "lesma",
    "displayName": "Lesma",
    "description": "Lesma Language Support",
    "version": "0.2.2",
    "publisher": "hassanalinali",
    "repository": "https://github.com/hassanalinali/Lesma-vscode",
    "author": "Alin Ali Hassan",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "lesma",
                "aliases": [
                    "Lesma",
                    "lesma"
                ],
                "extensions": [
                    ".les"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "lesma",
                "scopeName": "source.lesma",
                "path": "./syntaxes/lesma.tmLanguage.json"
            }
        ]
    }
}
